{
  "kind": "collectionType",
  "collectionName": "stats",
  "info": {
    "singularName": "stat",
    "pluralName": "stats",
    "displayName": "Stat"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "match": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::match.match",
      "inversedBy": "stats"
    },
    "rank": {
      "min": 0,
      "required": true,
      "type": "integer"
    },
    "eliminations": {
      "type": "integer",
      "required": true,
      "min": 0
    },
    "win": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "stats"
    },
    "team": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::team.team",
      "inversedBy": "stats"
    }
  }
}
