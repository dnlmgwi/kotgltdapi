{
  "kind": "collectionType",
  "collectionName": "teams",
  "info": {
    "singularName": "team",
    "pluralName": "teams",
    "displayName": "Team",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "icon": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "invite_code": {
      "type": "uid",
      "default": "invite_code",
      "minLength": 9,
      "required": true
    },
    "captain": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "mappedBy": "created_team"
    },
    "deleted": {
      "type": "boolean",
      "default": false
    },
    "team_name": {
      "type": "string",
      "minLength": 3,
      "required": true
    },
    "stats": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::stat.stat",
      "mappedBy": "team"
    },
    "team_members": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::invite.invite",
      "mappedBy": "team"
    },
    "deleted_at": {
      "type": "datetime"
    }
  }
}
